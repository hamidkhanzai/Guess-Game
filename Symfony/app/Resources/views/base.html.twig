<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
{% stylesheets
    '@AcmeSepaBlogBundle/Resources/public/css/main.css'
%}
<link rel="stylesheet" href="{{ asset_url }}" />
{% endstylesheets %}
{% image '@AcmeSepaBlogBundle/Resources/public/images/guess.jpg'
   		 output='/images/guess.jpg'%}
	
{% endimage %}

</head>
<body >

	<div id="wrapper">
		<div id="header">
		
		
		<h1>{% block header %}{% endblock %}</h1>
		</div><!-- #header-->
	
		<div id="middle">
				
			<div id="container">
				<div id="content">
				
				  {% block body %}{% endblock %}	
				 {% block history %}{% endblock %} 
				</div><!-- #content-->
			</div><!-- #container-->
			<div align='left' id="sidebar">
				
				
				
				<h3>{%block menue %}{% endblock %}<h3>
				
				
				
				
			</div><!-- #sidebar -->
			<div id="sidebarR">
			{% block top10 %}{% endblock %}
			</div>
		</div><!-- #middle-->
	</div><!-- #wrapper -->
	
	<div id="footer">
		<div class="langbar">
		</div>
		
	</div><!-- footer  -->
</body>

</html>
